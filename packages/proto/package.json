{
  "name": "@gotabit/proto",
  "version": "1.1.2",
  "description": "Typescript Protobuf Messages for GotaBit",
  "author": "GotaBit DEV <dev@gotabit.org>",
  "homepage": "https://github.com/gotabit/sdk-ts",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "_moduleAliases": {
    "~proto": "dist"
  },
  "scripts": {
    "build:module": "cross-env BABEL_ENV=production babel src --out-dir dist --delete-dir-on-start --extensions \".tsx,.ts,.js\"",
    "build": "npm run build:module",
    "build:proto": "npm run clean && npm run codegen",
    "codegen": "cross-env NODE_ENV=development babel-node scripts/codegen.js",
    "build:ts": "tsc --project ./tsconfig.json",
    "clean": "rimraf ./types && rimraf ./src/codegen",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gotabit/sdk-ts"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/gotabit/sdk-ts/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/generator": "7.19.0",
    "@babel/node": "^7.19.1",
    "@babel/parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-runtime": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/traverse": "7.19.1",
    "@babel/types": "7.19.0",
    "@osmonauts/telescope": "0.26.0",
    "@protobufs/cosmos": "^0.0.11",
    "@protobufs/cosmwasm": "^0.0.11",
    "@protobufs/ibc": "^0.0.11",
    "@types/jest": "^29.0.2",
    "ast-stringify": "0.1.0",
    "axios": "^0.27.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.0.3",
    "babel-watch": "^7.0.0",
    "case": "1.6.3",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "8.0.3",
    "jest": "^29.0.3",
    "jest-in-case": "^1.0.2",
    "link-module-alias": "^1.2.0",
    "mkdirp": "1.0.4",
    "prettier": "^2.7.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "3.0.2",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "@cosmjs/amino": "0.29.2",
    "@cosmjs/proto-signing": "0.29.2",
    "@cosmjs/stargate": "0.29.2",
    "@cosmjs/tendermint-rpc": "^0.29.2",
    "@osmonauts/helpers": "^0.5.0",
    "long": "^5.2.0",
    "protobufjs": "^6.11.3"
  }
}
